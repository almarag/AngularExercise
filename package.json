{
  "name": "angularexcercises",
  "version": "1.0.0",
  "description": "Basic Excercise",
  "main": "index.js",
  "scripts": {
    "build": "sass ./src/scss/styles.scss ./src/css/custom-styles.css --style compressed && parcel build ./wwwroot/index.html --no-source-maps",
    "start": "sass ./src/scss/styles.scss ./src/css/custom-styles.css --style compressed && parcel ./wwwroot/index.html --open --port 8080",
    "lint": "tslint src/**/*.ts src/**/*.tsx",
    "test": "protractor ./src/Tests/conf.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/angular": "^1.6.54",
    "angular": "^1.7.8",
    "angular-route": "^1.7.8",
    "bootstrap": "^4.3.1",
    "global": "^4.3.2",
    "jquery": "^3.3.1",
    "sass": "^1.17.3"
  },
  "devDependencies": {
    "@types/angular-route": "^1.7.0",
    "@types/bootstrap": "^4.3.0",
    "@types/jquery": "^3.3.29",
    "browser-sync": "^2.26.3",
    "gulp": "^4.0.0",
    "gulp-sass": "^4.0.2",
    "jasmine": "^3.3.1",
    "parcel": "^1.12.3",
    "parcel-plugin-static-files-copy": "^2.0.0",
    "tslint": "^5.15.0",
    "typescript": "^3.4.1"
  },
  "parcelCleanPaths": [
    "dist"
  ],
  "staticFiles": {
    "staticPath": "wwwroot",
    "watcherGlob": "**"
  }
}
